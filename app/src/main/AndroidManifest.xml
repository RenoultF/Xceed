        android:icon="@drawable/logo_xceed"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_xceed"
        android:supportsRtl="true"
        android:theme="@style/Theme.Xceed">
        <activity
            android:name=".SplashActivity"
            android:label="@string/app_name"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity android:name=".MainActivity"/>
        <activity android:name=".ui.exercice.ExerciceActivity"/>
        <activity android:name=".ui.entrainement.EntrainementActivity"/>
        <activity android:name=".ui.entrainement.Entrainement2Activity"/>
    </application>

</manifest>